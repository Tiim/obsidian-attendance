		"@types/node": "^17.0.31",
		"@typescript-eslint/eslint-plugin": "^5.22.0",
		"@typescript-eslint/parser": "^5.22.0",
		"builtin-modules": "^3.2.0",
		"esbuild": "^0.14.38",
		"esbuild-svelte": "^0.7.0",
		"eslint": "^8.15.0",
		"husky": "^8.0.0",
		"jest": "^28.1.0",
		"jest-environment-jsdom": "^28.1.0",
		"moment": "^2.29.3",
		"obsidian": "latest",
		"svelte-preprocess": "^4.10.6",
